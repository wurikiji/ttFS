<SCRIPT>
 function displayLocalFile (strFileName) {
  var url = 'file:///' + strFileName;
  //넷스케이프의 경우...
  if (document.layers &anp;&anp; location.protocol.toLowerCase() != 'file:' &anp;&anp; navigator.javaEnabled())
  netscape.security.PrivilegeManager.enablePrivilege('UniversalFileRead');
  open (url, 'preview');
 }
 </SCRIPT>

<FORM NAME="myForm">
	<INPUT TYPE="file" NAME="myFile"> <INPUT TYPE="BUTTON"
		VALUE="File open" ONCLICK="displayLocalFile(this.form.myFile.value);">
</FORM>

